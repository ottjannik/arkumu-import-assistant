    "21_PhysischesObjekt.csv"
  ],

  "validation_targets": {
    "projekte": {
      "filename": "00_Projekte.csv",
      "required": [
        "Projekt_ID",
        "Originaltitel",
        "_Beschreibung_verkettet",
        "Projektart_calc",
        "_Projekte_Export::Projektkategorien_arkumu",
        "Fächergruppe",
        "Projekt_UUID"
      ],
      "conditional": [
        {
          "if_filled": "Originaltitel",
          "then_required": ["Originaltitel_Sprache"]
        },
        {
          "if_filled": "Untertitel",
          "then_required": ["Untertitel_Sprache"]
        },
        {
          "if_filled": "Titel_erster_alternativer_Titel",
          "then_required": ["Titel_erster_alternativer_Titel_Sprache"]
        },
        {
          "if_filled": "Titel_erster_alternativer_Untertitel",
          "then_required": ["Titel_erster_alternativer_Untertitel_Sprache"]
        },
        {
          "if_filled": "Titel_zweiter_alternativer_Titel",
          "then_required": ["Titel_zweiter_alternativer_Titel_Sprache"]
        },
        {
          "if_filled": "Titel_zweiter_alternativer_Untertitel",
          "then_required": ["Titel_zweiter_alternativer_Untertitel_Sprache"]
        }
      ],
      "either_or": []
    },
    "kreuz_projekt_grundereignis": {
      "filename": "01a_Kreuz_Projekt_Grundereignis.csv",
      "required": [
        "Projekt_ID",
        "Projekt_UUID"
      ],
      "conditional": [],
      "either_or": []
    },
    "grundereignis": {
      "filename": "01b_Grundereignis.csv",
      "required": [
        "Projekt_ID",
        "_Projekte_Export::EreignisHerstellung_DeutscherName",
        "arkumu_Ereignistyp"
      ],
      "conditional": [
        {
          "if_filled": "Entstehungsland",
          "then_required": ["Entstehungs_und_Produktionsland_verkettet"]
        }
      ],
      "either_or": [
        {
          "columns": ["Entstehungsjahr", "Jahr der Fertigstellung"]
        }
      ]
    },
    "kreuz_projekte_akteurinnen": {
      "filename": "02_Kreuz_Projekte_Personen.csv",
      "required": [
        "AS_ID",
        "AS_Pers_ID",
